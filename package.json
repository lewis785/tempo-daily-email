    "coverage": "jest --coverage --config jestconfig.json",
    "gitversion": "git add -A src",
    "postgitversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lewis785/tempo-summary-email.git"
  },
