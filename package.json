  "dependencies": {
    "jira-client": "^6.14.0",
    "tempo-client": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/jira-client": "^6.13.1",
    "@types/request-promise": "^4.1.45",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  }
