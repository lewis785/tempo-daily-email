{
  "name": "tempo-daily-email",
  "version": "0.2.0",
  "description": "Generates a daily email for work completed in jira. Use Tempo and Jira api's",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "tslint -p tsconfig.json",
    "lint-fix": "tslint --fix -p tsconfig.json",
    "start": "tsc && node ./build/index.js",
    "test": "jest --config jestconfig.json",
    "coverage": "jest --coverage --config jestconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lewis785/tempo-daily-email.git"
  },
  "keywords": [
    "Tempo",
    "Jira"
  ],
  "author": "Lewis McNeill",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lewis785/tempo-daily-email/issues"
  },
  "homepage": "https://github.com/lewis785/tempo-daily-email#readme",
  "dependencies": {
    "jira-client": "^6.12.0",
    "tempo-client": "^0.1.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/jira-client": "^6.4.6",
    "@types/request-promise": "^4.1.44",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  }
}
